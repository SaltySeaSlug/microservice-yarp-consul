{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Debug"
    }
  },
  "AllowedHosts": "*",
  "consul": {
    "host": "consul-server",
    "port": 8500,
    "discovery": {
      "instanceId": "food-cluster",
      "serviceName": "food-cluster",
      "enabled": true,
      "register": true
    }
  },
  "Jwt": {
    "Issuer": "https://localhost/",
    "Audience": "https://localhost/",
    "Key": "This is a sample secret key - please don't use in production environment."
  }
}
